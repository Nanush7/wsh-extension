{
  "compilerOptions": {
    "outDir": "./build/scripts",
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  },
  "include": ["./scripts/**/*"]
}